<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<DialogPane
        prefHeight="150.0" prefWidth="280.0"
        xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <Label fx:id="title" alignment="CENTER" prefHeight="50.0" prefWidth="280.0" text="Title">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </header>
   <content>
      <AnchorPane prefHeight="80.0" prefWidth="280.0">
         <children>
            <Button id="dialog_play" alignment="CENTER" layoutX="33.0" layoutY="22.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="新游戏" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="10.0">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button id="dialog_back" alignment="CENTER" layoutX="176.0" layoutY="22.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="80.0" text="返回首页" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="10.0">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </content>
</DialogPane>
